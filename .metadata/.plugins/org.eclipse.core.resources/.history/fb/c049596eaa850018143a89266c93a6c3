import java.util.Scanner;
public class LP {

	public static void main(String[] args) {
		int num,i;
		  Scanner scan = new Scanner(System.in);
			System.out.println("Enter a Number : ");
	        num = scan.nextInt();
	        
	        for(int i=1;i<=num;i++){
				for(int j=1;j<=i;j++){
					System.out.print(" "+i);
				}
	        }

	}

}
